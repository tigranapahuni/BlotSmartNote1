<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:background="@color/coloryellownew"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/line1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/x_48" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/s_48" />

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/xar_48" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/q_48" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/t_48" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/line18"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@drawable/btn_score"
                android:gravity="center"
                android:text="K"
                android:ems="3"
                android:textColor="@color/coloryellownew"
                android:textSize="40sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@drawable/btn_score"
                android:gravity="center"
                android:ems="3"
                android:text="2x"
                android:textColor="@color/coloryellownew"
                android:textSize="40sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@drawable/btn_score"
                android:gravity="center"
                android:ems="3"
                android:text="4x"
                android:textColor="@color/coloryellownew"
                android:textSize="40sp"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/line2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="1"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="2"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="3"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/line3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="4"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="5"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="6"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/line10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="7"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="8"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="9"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/line6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="<-"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_margin="1dp"
                android:text="0"
                android:ems="3"
                android:textStyle="bold"
                android:textColor="@color/coloryellownew"
                android:background="@drawable/btn_score"
                android:textSize="40sp"/>



        </LinearLayout>

    </LinearLayout>

</RelativeLayout>